<script type="text/javascript">

  jQuery.fn.center = function() {
    parent = this.parent();
    this.css({
      "position": "absolute",
      "top": (((jQuery(parent).height() - this.outerHeight()) / 2) + jQuery(parent).scrollTop() + "px"),
      "left": (((jQuery(parent).width() - this.outerWidth()) / 2) + jQuery(parent).scrollLeft() + "px")
    });
    return this;
  }
  
</script>
