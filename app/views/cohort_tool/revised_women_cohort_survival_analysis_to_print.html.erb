<script language="javascript" type="text/javascript" src="/javascripts/jquery-1.3.2.min.js"></script>


<style type="text/css" xmlns="http://www.w3.org/1999/html">
    td {
        padding: 12px;
    }
    .table-main {
        width: 99%;
        height: auto;
        border: 0;
        border-spacing: 0;
        font-family: "Helvetica Neue", Arial, "Lucida Grande", sans-serif;
        font-size: smaller;
        overflow: auto;
        text-overflow: ellipsis;
        margin: auto;
        page-break-inside: auto;
        padding-top: 20px;
        padding-bottom: 70px;
    }
    .table-inner {
        width: 99%;
    }
    .border-null {
        border: none;
    }
    .text-bold {
        font-weight: bold;
    }
    .text-center {
        text-align: center;
    }
    .text-italic {
        font-style: italic;
    }
    .text-smaller {
        font-size: smaller;
    }
    .text-left {
        text-align: left;
    }
    .index {
        border: none;
        width: 2%;
    }
    .border-null-right {
        border-right: none;
    }
    .border-null-left {
        border-left: none;
    }
    .border-null-top {
        border-top: none;
    }
    .border-null-bottom {
        border-bottom: none;
    }
    .border-left-dotted {
        border-left-style: dotted;
    }
    .border-right-dotted {
        border-right-style: dotted;
    }
    .border-bottom-dotted {
        border-bottom-style: dotted;
    }
    .border-top-dotted {
        border-top-style: dotted;
    }
    .button-print {
        background-color: green;
        font-size: 54px;
        color: white;
        float: right;
        border: 5px solid grey;
        padding-left: 20px;
        padding-right: 20px;
        border-radius: 25px;
    }
    .row-button-print {
        position: fixed;
        left: 0px;
        bottom: 0px;
        width: 100%;
        background-color: rgb(51,51,51);
    }
    .column-break {
        width: 1%;
        border: none;
    }
    .bolder {
        border: black;
        border-style: solid;
        border-width: 2px;
    }
    .bolder-left {
        border-left: black;
        border-left-style: solid;
        border-left-width: 2px;
    }
    .bolder-right {
        border-right: black;
        border-right-style: solid;
        border-right-width: 2px;
    }
    .bolder-bottom {
        border-bottom: black;
        border-bottom-style: solid;
        border-bottom-width: 2px;
    }
    .bolder-top {
        border-top: black;
        border-top-style: solid;
        border-top-width: 2px;
    }
    .number {
        text-align: right;
    }
    .number-center {
        text-align: center;
    }
    .table-inner-td {
        width: 10% !important;
    }
    .table-inner-data {
        width: 10% !important;
    }
    .text-header {
        font-size: large;
    }
    .logo {
        float:left
    }
</style>

<div>
    <%= image_tag(@logo, :class => "logo")%>
</div>

<table class="table-main" border="2px;">
    <tr>
        <th colspan="10" class="text-left border-null text-header">
            Women Cohort Survival Analysis
        </th>
    </tr>
    <tr>
        <td class="border-null-right">
            Reg. cohort
        </td>
        <td class="border-null-left border-null-right">
            Interval months
        </td>
        <td class="border-null-left border-null-right">
            Sub group
        </td>
        <td class="border-null-left border-null-right">
            Total reg. (database)
        </td>
        <td class="border-null-left">
            Total reg. (confirmed)
        </td>
        <td colspan="5">
            <table class="table-inner">
                <tr>
                    <td colspan="5" class="text-center">
                        Outcomes as of end of quarter evaluated
                    </td>
                </tr>
                <tr>
                    <td class="table-inner-td text-center">
                        Alive
                    </td>
                    <td class="table-inner-td text-center">
                        Died
                    </td>
                    <td class="table-inner-td text-center">
                        Defaulted
                    </td>
                    <td class="table-inner-td text-center">
                        Stopped
                    </td>
                    <td class="table-inner-td text-center">
                        Transfer out
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    <% @name_of_quarter = @quarter.split.first %>
    <%  (@cohort.women_survival_analysis.sort || []).each do |range, data| %>
        <% if data['Number of new registered'].to_i > 1 %>
            <tr>
                <td class="text-bold border-null-right table-main-data">
                    <%= "#{@name_of_quarter} " + data['Quarter'] %>
                </td>
                <td class="number-center border-null-left border-null-right table-main-data">
                    <%= range %>
                </td>
                <td class="border-null-left table-main-data">
                    all ages
                </td>
                <td class="number table-main-data">
                    <%= data['Number of new registered']%>
                </td>
                <td class="number table-main-data">

                </td>
                <td class="number table-inner-data">
                    <%= data['Number Alive and on ART']%>
                </td>
                <td class="number table-inner-data">
                    <%= data['Number Dead']%>
                </td>
                <td class="number table-inner-data">
                    <%= data['Number Defaulted']%>
                </td>
                <td class="number table-inner-data">
                    <%= data['Number Stopped Treatment']%>
                </td>
                <td class="number table-inner-data">
                    <%= data['Number Transferred out']%>
                </td>
            </tr>
        <% end %>
    <% end %>
</table>